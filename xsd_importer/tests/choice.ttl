@prefix i14y: <https://www.i14y.admin.ch/resources/datasets/dataset_identifier/structure/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

i14y:city a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "city"@en ;
    sh:path i14y:city .

i14y:name a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "name"@en ;
    sh:path i14y:name .

i14y:state a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "state"@en ;
    sh:path i14y:state .

i14y:street a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "street"@en ;
    sh:path i14y:street .

i14y:zip a sh:PropertyShape ;
    sh:datatype xsd:decimal ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "zip"@en ;
    sh:path i14y:zip .

