@prefix i14y: <http://i14y.admin.ch/ns#/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

i14y:irisShape a sh:NodeShape ;
    sh:closed true ;
    sh:name "iris" ;
    sh:property i14y:petal_length,
        i14y:petal_width,
        i14y:sepal_length,
        i14y:sepal_width,
        i14y:variety .

i14y:petal_length a sh:PropertyShape ;
    sh:datatype xsd:decimal ;
    sh:maxInclusive 6.9 ;
    sh:minInclusive 1.0 ;
    sh:name "petal.length" ;
    sh:path i14y:petal_length .

i14y:petal_width a sh:PropertyShape ;
    sh:datatype xsd:decimal ;
    sh:maxInclusive 2.5 ;
    sh:minInclusive 0.1 ;
    sh:name "petal.width" ;
    sh:path i14y:petal_width .

i14y:sepal_length a sh:PropertyShape ;
    sh:datatype xsd:decimal ;
    sh:maxInclusive 7.9 ;
    sh:minInclusive 4.3 ;
    sh:name "sepal.length" ;
    sh:path i14y:sepal_length .

i14y:sepal_width a sh:PropertyShape ;
    sh:datatype xsd:decimal ;
    sh:maxInclusive 4.4 ;
    sh:minInclusive 2.0 ;
    sh:name "sepal.width" ;
    sh:path i14y:sepal_width .

i14y:variety a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:name "variety" ;
    sh:path i14y:variety .

